<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.wjj.qrcpcheck.common.dao.AccountDAO">
    <resultMap id="AccountEntity" type="org.wjj.qrcpcheck.common.dao.AccountEntity">
        <id property="id" column="ID" jdbcType="VARCHAR" />
        <result property="uid" column="UID" jdbcType="VARCHAR"/>
        <result property="money" column="MONEY" jdbcType="VARCHAR"/>
    </resultMap>

    <select id = "getById" resultMap="AccountEntity">
        SELECT ID,UID,MONEY from ACCOUNT WHERE ID = #{id}
    </select>
</mapper>